<fieldset>
<%= form_for custom_project_window, :url => project_user_wl_custom_project_window_path do |f| %>

	<% if custom_project_window.errors.any? %>
		<div class="flash error">
			<ul>
		  		<% custom_project_window.errors.full_messages.each do |msg| %>
		    		<li><%= msg %></li>
		  		<% end %>
			</ul>
		</div>
	<% end %>

<p>Start: <%= f.text_field :start_date %><%= calendar_for('wl_custom_project_window_start_date') %>
			End: <%= f.text_field :end_date %><%= calendar_for('wl_custom_project_window_end_date') %></p>
	<p><%= f.submit 'Save' %> <a href="javascript:history.back()"><%= l(:button_back) %></a></p>
<% end %>
</fieldset>